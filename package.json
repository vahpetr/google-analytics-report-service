{
    "name": "google-analytics-report-service",
    "version": "1.0.0",
    "description": "Google analytics report service",
    "keywords": [
        "google analytics",
        "ga",
        "google report",
        "reporting",
        "csv",
        "web api",
        "oauth2",
        "jwt",
        "typescript",
        "nestjs",
        "jest",
        "e2e",
        "docker"
    ],
    "license": "apache-2.0",
    "private": true,
    "engines": {
        "node": ">=10.7.0"
    },
    "engineStrict": true,
    "scripts": {
        "dev": "ts-node -r tsconfig-paths/register src/main.ts",
        "dev:watch": "nodemon",
        "prestart:prod": "rm -rf dist && tsc",
        "prod": "node dist/main.js",
        "prod:bundle": "node scripts/build.js",
        "run:prod:bundle": "node bundle/server.js",
        "prod:analys": "node scripts/build.js && source-map-explorer bundle/server.js bundle/server.js.map",
        "test": "EXPIRES_IN=3600 SECRET_OR_KEY=secret BI_LOGIN=login BI_PASS=pass ENABLE_AUTH=0 jest",
        "test:cov": "jest --coverage",
        "update": "ncu -u",
        "reinstall": "rimraf node_modules && npm install",
        "get:credentials": "ts-node -r tsconfig-paths/register src/reports/getCredentials.ts",
        "lint": "tslint -c tslint.json --project tsconfig.json src/**/*.ts -t verbose"
    },
    "dependencies": {
        "@nestjs/common": "5.1.0",
        "@nestjs/core": "5.1.0",
        "@nestjs/microservices": "5.1.0",
        "@nestjs/testing": "5.1.0",
        "@nestjs/websockets": "5.1.0",
        "@types/express": "4.16.0",
        "@types/passport": "0.4.6",
        "@types/passport-jwt": "3.0.1",
        "app-root-path": "2.1.0",
        "axios": "0.18.0",
        "babel-jest": "23.4.0",
        "babel-loader": "7.1.5",
        "case-sensitive-paths-webpack-plugin": "2.1.2",
        "chalk": "2.4.1",
        "depd": "1.1.2",
        "dotenv": "6.0.0",
        "duplicate-package-checker-webpack-plugin": "3.0.0",
        "express": "4.16.3",
        "file-loader": "1.1.11",
        "fork-ts-checker-webpack-plugin": "0.4.3",
        "fs-extra": "7.0.0",
        "googleapis": "32.0.0",
        "isarray": "2.0.4",
        "jest": "23.4.1",
        "json2csv": "4.1.6",
        "jsonwebtoken": "8.3.0",
        "object-assign": "4.1.1",
        "opn": "5.3.0",
        "passport": "0.4.0",
        "passport-jwt": "4.0.0",
        "promise": "8.0.1",
        "raf": "3.4.0",
        "react-dev-utils": "5.0.1",
        "readable-stream": "2.3.6",
        "reflect-metadata": "0.1.12",
        "rxjs": "6.2.2",
        "setprototypeof": "1.1.0",
        "source-map-loader": "0.2.3",
        "string_decoder": "1.1.1",
        "ts-jest": "23.0.1",
        "ts-loader": "4.4.2",
        "ts-node": "7.0.0",
        "tsconfig-paths": "3.4.2",
        "tsconfig-paths-webpack-plugin": "3.2.0",
        "tslint": "5.11.0",
        "tslint-loader": "3.6.0",
        "tslint-sonarts": "1.7.0",
        "typescript": "2.9.2",
        "uglify-js": "3.4.5",
        "uglifyjs-webpack-plugin": "1.2.7",
        "url-loader": "1.0.1",
        "webpack": "4.16.1",
        "whatwg-fetch": "2.0.4"
    },
    "devDependencies": {
        "@types/jest": "23.3.0",
        "@types/json2csv": "4.1.0",
        "@types/node": "10.5.2",
        "@types/opn": "5.1.0",
        "@types/supertest": "2.0.5",
        "nodemon": "1.18.3",
        "npm-check-updates": "2.14.2",
        "rimraf": "2.6.2",
        "source-map-explorer": "1.5.0",
        "stats-webpack-plugin": "0.6.2",
        "supertest": "3.1.0"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}"
        ],
        "moduleDirectories": [
            "node_modules",
            "."
        ],
        "moduleFileExtensions": [
            "mjs",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "web.js",
            "js",
            "web.jsx",
            "jsx",
            "json",
            "node"
        ],
        "testMatch": [
            "<rootDir>/src/**/?(*.)(spec).(j|t)s?(x)",
            "<rootDir>/test/**/?(*.)(e2e-spec).(j|t)s?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            ".+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            ".+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
            ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
        ],
        "globals": {
            "ts-jest": {
                "tsConfigFile": "tsconfig.test.json"
            }
        }
    }
}
